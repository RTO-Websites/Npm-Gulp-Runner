{
  "Targets": [
    {
      "Mode": "Dev",
      "Path": "./"
    },
    {
      "Mode": "Build",
      "Path": "./build/"
    },
    {
      "Mode": "CiBuild",
      "Path": "./CiBuild/"
    }
  ],
  "Types": [
    {
      "Name": "SCSS",
      "Src": "SCSS/**/*.scss",
      "Dest": "CSS"
    },
    {
