      "Dest": "Assets"
    },
    {
      "Name": "CoffeeScript",
      "Src": "Coffee/**/*.",
      "Dest": "CSS"
    },
    {
      "Name": "Vendor",
      "Src": "Vendor",
      "Dest": "Vendor"
    },
    {
      "Name": "Vendor",
      "Src": "Vendor",
      "Dest": "Vendor"
    }
  ],
  "Folders": [
    {
      "Src": "Admin",
      "Dest": "Admin",
      "Types": ["SCSS", "JS"]
    },
    {
      "Src": "Public",
      "Dest": "Public",
      "Types": ["SCSS", "JS", "Static"]
    }
  ]
